{
  "version": "1.0.1",
  "license": "MIT",
  "name": "numeric-quantity",
  "author": "Jake Boone <jakeboone02@gmail.com>",
  "description": "Number parser with support for mixed numbers and vulgar fractions",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/numeric-quantity.esm.js",
  "unpkg": "dist/numeric-quantity.umd.production.min.js",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/jakeboone02/numeric-quantity/issues"
  },
  "homepage": "https://github.com/jakeboone02/numeric-quantity",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakeboone02/numeric-quantity.git"
  },
  "keywords": [
    "convert",
    "number",
    "fraction",
    "decimal"
  ],
  "scripts": {
    "start": "tsdx watch --format cjs,esm,system,umd",
    "build": "tsdx build --format cjs,esm,system,umd",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "publish:npm": "np"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "devDependencies": {
    "np": "^7.3.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  }
}
