      "import": "./dist/numeric-quantity.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/numeric-quantity.d.ts",
  "unpkg": "./dist/numeric-quantity.global.js",
  "bugs": {
    "url": "https://github.com/jakeboone02/numeric-quantity/issues"
  },
  "homepage": "https://github.com/jakeboone02/numeric-quantity",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakeboone02/numeric-quantity.git"
  },
  "keywords": [
    "convert",
    "number",
    "fraction",
    "decimal"
  ],
  "scripts": {
    "start": "vite",
    "build": "tsup",
    "test": "jest",
    "watch": "jest --watch",
    "publish:npm": "np",
    "pretty-print": "prettier --write *.{mjs,js,ts,json} src/*.*"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "np": "^8.0.2",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
